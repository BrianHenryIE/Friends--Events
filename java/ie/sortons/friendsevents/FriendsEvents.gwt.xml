<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='friendsevents'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<inherits name='com.google.gwt.user.theme.clean.Clean' />


	<inherits name='com.google.web.bindery.event.EventBinder' />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name="com.google.gwt.resources.Resources" />

	<inherits name="ie.brianhenry.gwtbingmaps.GwtBingMaps" />
	<inherits name="ie.sortons.gwtfbplus.GwtFbPlus" />

	<inherits name='com.kfuntak.gwt.json.serialization.GWTProJsonSerializer' />

	<!-- Specify the app entry point class. -->
	<entry-point class='ie.sortons.friendsevents.client.FriendsEvents' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>
